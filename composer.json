{
  "name": "baoziyoo/hyperf-dto",
  "description": "php hyperf dto",
  "license": "MIT",
  "keywords": [
    "dto",
    "hyperf",
    "hyperf dto"
  ],
  "authors": [
    {
      "name": "TangWei",
      "email": "tw2066@163.com",
      "homepage": "https://github.com/tw2066",
      "role": "Developer"
    },
    {
      "name": "ogg",
      "email": "baoziyoo@gmail.com",
      "homepage": "https://github.com/baoziyoo",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "netresearch/jsonmapper": "~4.0.0",
    "hyperf/http-server": "~3.0.0",
    "hyperf/di": "~3.0.0",
    "hyperf/validation": "~3.0.0",
    "phpdocumentor/reflection-docblock": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": ">=7.0",
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Baoziyoo\\Hyperf\\DTO\\": "src/"
    }
  },
  "extra": {
    "hyperf": {
      "config": "Baoziyoo\\Hyperf\\DTO\\ConfigProvider"
    },
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix src"
  }
}
